blockquote.quote p { display: flex; font-style: italic; }

blockquote.quote p .icon { margin-right: 1rem; margin-top: 0.2rem; }

blockquote.quote .name { display: inline; font-size: var(--type-base-font-size-rem); margin: 0px; }

main.contribute { margin-bottom: 3rem; width: 100%; }

main.contribute .stats-container { background-color: var(--background-primary); color: var(--text-primary); margin-bottom: 3rem; width: 100%; }

main.contribute section { margin: 0px auto; max-width: 52rem; padding: 0px 1rem; }

main.contribute section.stats-header { align-items: center; display: flex; flex-direction: column; padding: 0px 0.5rem 2rem; }

main.contribute section h1 { font-size: 3rem; margin-left: 0.5rem; margin-top: 8rem; text-align: center; }

main.contribute section h1::before { background-color: var(--text-link); content: ""; display: inline-block; height: 1.2em; -webkit-mask: url("/static/media/m-logo.e19e04547be91a35c8ea.svg") 50% 50% / cover no-repeat; transform: translate(-0.2em, 0.2em); width: 1.2em; }

main.contribute section h1::after { content: " "; text-decoration-line: underline; text-decoration-style: initial; text-decoration-color: var(--text-link); text-decoration-thickness: 0.1em; text-underline-offset: 0.1em; }

main.contribute section .quote.owd { background-color: var(--text-link); color: var(--background-primary); }

