body { margin: 0px; padding: 0px; background: rgb(0, 0, 0); font-family: Tahoma, Verdana, Arial, Helvetica, sans-serif; font-size: 12px; color: rgb(135, 135, 135); }

h1, h2, h3 { margin: 0px; padding: 0px; font-family: Montserrat, sans-serif; font-weight: normal; color: rgb(16, 16, 16); }

h2 { padding-bottom: 10px; letter-spacing: -1px; font-size: 2em; }

p, ol, ul { margin-top: 0px; }

p { line-height: 180%; }

strong { }

a { color: rgb(57, 194, 240); }

a:hover { text-decoration: none; }

img { margin: 10px 10px 10px 0px; display: block; max-width: 100%; }

a img { border: none; }

img.border { }

img.alignleft { float: left; }

img.alignright { float: right; }

img.aligncenter { margin: 0px auto; }

hr { display: none; }

iframe, object { width: 100%; height: 480px; }

.breadcrumb { padding: 0px 0px 15px; }

table { border-collapse: collapse; }

td, th { }

#wrapper { width: 90%; padding: 0px 50px; background: rgb(255, 255, 255); }

.container { width: 100%; margin: 0px auto; }

.clearfix { clear: both; }

#header { overflow: hidden; padding: 30px 0px 0px; }

#logo { float: left; width: 400px; margin: 0px; padding: 0px; }

#logo h1, #logo p { }

#logo h1 { letter-spacing: -2px; font-size: 3em; }

#logo h1 a { color: rgb(16, 16, 16); text-transform: uppercase; }

#logo p { margin: 0px; padding: 0px; color: rgb(142, 142, 142); }

#logo p a { color: rgb(0, 0, 0); }

#logo a { border: none; background: none; text-decoration: none; color: rgb(18, 33, 47); }

#menu1 { float: right; width: 700px; margin: 0px auto; padding: 0px; }

#menu1 ul { float: right; margin: 0px; padding: 10px 0px 0px; list-style: none; line-height: normal; }

#menu1 li { float: left; padding: 0px; }

#menu1 a { display: block; margin-right: 1px; padding: 10px 15px; text-decoration: none; text-align: center; text-transform: uppercase; font-family: Montserrat, sans-serif; font-size: 13px; font-weight: 300; color: rgb(88, 88, 88); border: none; }

#menu1 .current_page_item a { text-decoration: none; color: rgb(255, 255, 255); }

#menu1 .current_page_item a { background: rgb(42, 168, 210); border-radius: 5px; }

#page { overflow: hidden; padding: 50px 0px 0px; border-top: 1px solid rgb(222, 222, 222); }

#content { float: left; width: 100%; }

#content #cbox1 { margin-bottom: 40px; }

#sidebar { float: right; width: 300px; }

#sidebar #box1 { margin-bottom: 50px; }

#footer { overflow: hidden; padding: 50px 0px; border-top: 1px solid rgb(222, 222, 222); }

#footer p { text-align: center; font-size: 12px; }

#footer a { }

#banner { overflow: hidden; }

#two-column { overflow: hidden; padding: 40px 0px; border-top: 1px solid rgb(222, 222, 222); }

#two-column h2 { font-size: 1.5em; }

#two-column #tbox1 { float: left; width: 350px; }

#two-column #tbox2 { float: right; width: 350px; }

.link-style { display: inline-block; margin-top: 20px; padding: 10px 20px; background: rgb(0, 0, 0); text-decoration: none; color: rgb(255, 255, 255); }

ul.style1 { margin: 0px; padding: 0px; list-style: none; }

ul.style1 li { padding: 10px 0px 15px; border-top: 1px solid rgb(230, 231, 220); }

ul.style1 .first { padding-top: 0px; border-top: none; }

ul.style2 { margin: 0px; padding: 0px; list-style: none; }

ul.style2 li { padding: 25px 0px 15px; border-top: 1px solid rgb(230, 231, 220); }

ul.style2 .first { padding-top: 0px; border-top: none; }

ul.style2 h3 { padding: 0px 0px 10px; }

ul.style2 h3 a { color: rgb(16, 16, 16); }

ul.style2 a { text-decoration: none; }

ul.style2 a:hover { text-decoration: underline; }

ul.style3 { margin: 0px; padding: 0px; list-style: none; }

ul.style3 li { padding: 20px 0px; border-top: 1px solid rgb(230, 231, 220); }

ul.style3 p { margin: 0px; padding: 0px; }

ul.style3 img { float: left; margin-top: 3px; margin-right: 20px; }

ul.style3 .posted { padding: 10px 0px; font-size: 8pt; color: rgb(162, 162, 162); }

ul.style3 .first { padding-top: 0px; border-top: none; }

ul.relpost { list-style: none; margin: 0px; padding: 0px; display: flex; justify-content: space-around; flex-wrap: wrap; text-align: center; align-items: flex-start; }

ul.relpost li { display: block; padding: 5px; margin: 5px; width: 200px; min-height: 210px; max-height: 250px; overflow: hidden; border: 2px solid rgb(102, 102, 102); border-radius: 5px; line-height: 1.4em; transition: border-color 0.2s ease 0s, color 0.2s ease 0s; }

ul.relpost li:hover { border-color: rgb(243, 70, 7); }

