ul.relpost li div.relimg { max-height: 150px; min-width: 100%; overflow: hidden; border-radius: 5px; display: block; height: 150px; }

.mylink { color: rgb(0, 114, 188); text-decoration: underline; cursor: pointer; }

.mylink:hover { color: rgb(226, 1, 2); }

.pagination > li { display: inline; padding: 10px; background: rgb(42, 168, 210); border-radius: 5px; margin: 2px; }

.pagination > li a { text-decoration: none; color: rgb(255, 255, 255); }

.clear { clear: both; }

@media screen and (max-width: 768px) {
  #banner { visibility: hidden; display: none; }
